{
  "name": "Browser Calculator Extension",
  "description": "Use calculator right from the browser. Created by Sumit Kumar Prasad",
  "version": "1.0",
  "manifest_version":3,
  "background": {
    "service_worker": "background.js"
  },

  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "1.png",
      "32": "1.png",
      "48": "1.png",
      "128": "1.png"
    }
},
"options_page": "options.html"
}
